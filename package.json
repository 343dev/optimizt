  },
  "type": "module",
  "engines": {
    "node": ">=18.17"
  },
  "dependencies": {
    "cli-progress": "^3.11.0",
    "commander": "^12.1.0",
    "exec-buffer": "^3.2.0",
    "fdir": "^6.4.0",
    "gif2webp-bin": "^5.0.0",
    "gifsicle": "^7.0.0",
    "guetzli": "^5.0.0",
    "p-limit": "^6.1.0",
    "sharp": "^0.33.4",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@343dev/eslint-config": "^1.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.8",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@343dev",
    "env": {
      "jest": true
    }
  }
