{
  "name": "@343dev/optimizt",
  "version": "8.0.1",
  "description": "CLI image optimization tool",
  "keywords": [
    "svg",
    "png",
    "jpeg",
    "gif",
    "webp",
    "avif",
    "image-compression",
    "sharp",
    "svgo",
    "gifsicle",
    "guetzli",
    "gif2webp"
  ],
  "license": "MIT",
  "author": "Andrey Warkentin (https://github.com/343dev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/343dev/optimizt"
  },
  "exports": "./index.js",
  "bin": {
    "optimizt": "./cli.js"
  },
  "files": [
    "MIGRATION.md",
    "cli.js",
    "convert.js",
    "index.js",
    "lib/",
    "optimize.js",
    ".optimiztrc.cjs"
  ],
  "scripts": {
    "lint": "npx eslint ./",
